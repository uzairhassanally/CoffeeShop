@model int



@{
    ViewBag.Title = "Checkout Complete";
    Layout = "~/Views/Shared/Customer.cshtml";
}
<h2>Checkout Complete</h2>
<p>Thanks for your order! Your order number is: @Model</p>
<p>
    How about shopping for some more Products in our
    @Html.ActionLink("store",
   "StoreIndex", "Customer")
</p>

<p>
    Click Here to see When Your Order Will Be Ready
    @Html.ActionLink("Orders",
   "OrderTable", "Customer")
</p>
<p>
    Click Here to see History Of Your Orders
    @Html.ActionLink("MyOrders",
   "CustomersOrder", "Customer")
</p>
